{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "routes": [
    {
      "route": "/*",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "same-origin"
      }
    }
  ]
} 