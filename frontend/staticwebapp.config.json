{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "X-Frame-Options": "",
    "Content-Security-Policy": "frame-ancestors 'self' https://dermaself-dev.myshopify.com https://*.myshopify.com https://admin.shopify.com",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "same-origin"
  }
}
